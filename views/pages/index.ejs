    <!-- ============= service Section ends here ======= -->
    <!-- ============= Testimonial Section start here ======= -->
    <section class="testimonials-section pb-2">
      <div class="container px-4 py-5">
        <h2 class="testimonials-title">
          What Our <br /><span class="px-4 text-primary">Guests say</span>
        </h2>
        <p class="testimonials-subtitle">
          Real reviews from our satisfied visitors.
        </p>
        <div class="swiper testimonials-swiper">
          <div class="swiper-wrapper">
            <!-- Testimonial 1 -->
            <div class="swiper-slide testimonials-slide">
              <div class="testimonial-card">
                <div class="testimonial-avatar">
                  <img
                    src="assets/images/profile-placeholder.webp"
                    alt="reviewer placeholder image"
                    height="80"
                    width="80"
                  />
                </div>
